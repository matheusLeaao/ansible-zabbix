<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2020-07-29T19:08:14Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template SQS - Mandic</template>
            <name>Template SQS - Mandic</name>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>SQS</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Approximate Age Of Oldest Message</name>
                    <type>EXTERNAL</type>
                    <key>cloudwatch-zabbix.py[&quot;ApproximateAgeOfOldestMessage&quot;,&quot;Maximum&quot;,&quot;QueueName&quot;,&quot;{HOST.HOST}&quot;,&quot;AWS/SQS&quot;,&quot;{$REGION}&quot;]</key>
                    <delay>5m</delay>
                    <value_type>FLOAT</value_type>
                    <applications>
                        <application>
                            <name>SQS</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Approximate Number Of Messages Delayed</name>
                    <type>EXTERNAL</type>
                    <key>cloudwatch-zabbix.py[&quot;ApproximateNumberOfMessagesDelayed&quot;,&quot;Average&quot;,&quot;QueueName&quot;,&quot;{HOST.HOST}&quot;,&quot;AWS/SQS&quot;,&quot;{$REGION}&quot;]</key>
                    <delay>300</delay>
                    <value_type>FLOAT</value_type>
                    <applications>
                        <application>
                            <name>SQS</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Approximate Number Of Messages Not Visible</name>
                    <type>EXTERNAL</type>
                    <key>cloudwatch-zabbix.py[&quot;ApproximateNumberOfMessagesNotVisible&quot;,&quot;Average&quot;,&quot;QueueName&quot;,&quot;{HOST.HOST}&quot;,&quot;AWS/SQS&quot;,&quot;{$REGION}&quot;]</key>
                    <delay>300</delay>
                    <value_type>FLOAT</value_type>
                    <applications>
                        <application>
                            <name>SQS</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Approximate Number Of Messages Visible</name>
                    <type>EXTERNAL</type>
                    <key>cloudwatch-zabbix.py[&quot;ApproximateNumberOfMessagesVisible&quot;,&quot;Average&quot;,&quot;QueueName&quot;,&quot;{HOST.HOST}&quot;,&quot;AWS/SQS&quot;,&quot;{$REGION}&quot;]</key>
                    <delay>300</delay>
                    <value_type>FLOAT</value_type>
                    <applications>
                        <application>
                            <name>SQS</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{last()}&gt;={$APPROXIMATE_NUMBER_OF_MESSAGES_VISIBLE}</expression>
                            <name>Approximate Number Of Messages Visible &gt; {$APPROXIMATE_NUMBER_OF_MESSAGES_VISIBLE}</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Number Of Empty Receives</name>
                    <type>EXTERNAL</type>
                    <key>cloudwatch-zabbix.py[&quot;NumberOfEmptyReceives&quot;,&quot;Sum&quot;,&quot;QueueName&quot;,&quot;{HOST.HOST}&quot;,&quot;AWS/SQS&quot;,&quot;{$REGION}&quot;]</key>
                    <delay>300</delay>
                    <value_type>FLOAT</value_type>
                    <units>count/s</units>
                    <applications>
                        <application>
                            <name>SQS</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Number Of Messages Deleted</name>
                    <type>EXTERNAL</type>
                    <key>cloudwatch-zabbix.py[&quot;NumberOfMessagesDeleted&quot;,&quot;Sum&quot;,&quot;QueueName&quot;,&quot;{HOST.HOST}&quot;,&quot;AWS/SQS&quot;,&quot;{$REGION}&quot;]</key>
                    <delay>300</delay>
                    <value_type>FLOAT</value_type>
                    <applications>
                        <application>
                            <name>SQS</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Number Of Messages Received</name>
                    <type>EXTERNAL</type>
                    <key>cloudwatch-zabbix.py[&quot;NumberOfMessagesReceived&quot;,&quot;Sum&quot;,&quot;QueueName&quot;,&quot;{HOST.HOST}&quot;,&quot;AWS/SQS&quot;,&quot;{$REGION}&quot;]</key>
                    <delay>5m</delay>
                    <value_type>FLOAT</value_type>
                    <applications>
                        <application>
                            <name>SQS</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Number Of Messages Sent</name>
                    <type>EXTERNAL</type>
                    <key>cloudwatch-zabbix.py[&quot;NumberOfMessagesSent&quot;,&quot;Sum&quot;,&quot;QueueName&quot;,&quot;{HOST.HOST}&quot;,&quot;AWS/SQS&quot;,&quot;{$REGION}&quot;]</key>
                    <delay>5m;5m/1-5,19:00-20:00</delay>
                    <value_type>FLOAT</value_type>
                    <applications>
                        <application>
                            <name>SQS</name>
                        </application>
                    </applications>
                </item>
            </items>
            <macros>
                <macro>
                    <macro>{$APPROXIMATE_NUMBER_OF_MESSAGES_VISIBLE}</macro>
                    <value>500</value>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
